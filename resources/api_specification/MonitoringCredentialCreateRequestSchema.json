{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha3" ]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "accessKey": {
          "type": "string"
        },
        "secretKey": {
          "type": "string"
        }
      },
      "required": ["accessKey","secretKey"]
    }
  },
  "required": ["apiVersion","metadata"]
}
