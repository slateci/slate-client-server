{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "required": true,
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha3" ]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
        },
        "group": {
          "type": "string",
        },
        "owningOrganization": {
          "type": "string",
        },
        "kubeconfig": {
          "type": "string",
        }
      },
      "required": ["name","group","organization","kubeconfig"]
    }
  },
  "required": ["apiVersion","metadata"]
}
