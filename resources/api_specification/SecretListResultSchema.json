{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha3" ]
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "apiVersion": {
            "type": "string",
            "enum": [
              "v1alpha3"
            ]
          },
          "kind": {
            "type": "string",
            "enum": [
              "Secret"
            ]
          },
          "metadata": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "id": {
                "type": "string"
              },
              "cluster": {
                "type": "string"
              },
              "group": {
                "type": "string"
              },
              "created": {
                "type": "string"
              }
            },
            "required": ["name","id","cluster","group","created"]
          }
        },
        "required": ["apiVersion","kind","metadata"]
        }
      }
    },
  "required": ["apiVersion","items"]
  }
}
