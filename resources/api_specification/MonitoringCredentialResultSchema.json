{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "properties": {
  "apiVersion": {
    "type": "string",
    "enum": [ "v1alpha3" ]
  },
  "kind": {
    "type": "string",
    "enum": [ "MonitoringCredential" ]
  },
  "metadata": {
    "type": "object",
    "properties": {
      "accessKey": {
        "type": "string",
      },
      "secretKey": {
        "type": "string",
      },
      "inUse": {
        "type": "boolean",
      },
      "revoked": {
        "type": "boolean",
      },
    },
    "required": ["accessKey","secretKey","inUse","revoked"]
  },
  "required": ["apiVersion","kind","metadata"]
}
